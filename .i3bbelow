separator_block_width=37
markup=none

[source]
label= OP:
command=/bin/bash $HOME/.libi3bview 24
color=#ffffff
separator=false
interval=1

[sink]
label= IP:
command=/bin/bash $HOME/.libi3bview 26
color=#ffffff
separator=false
interval=1

[usedram]
label= RAM:
command=/bin/bash $HOME/.libi3bview 21
color=#ffffff
separator=false
interval=1

[usedswap]
label= SWAP:
command=/bin/bash $HOME/.libi3bview 19
color=#ffffff
separator=false
interval=1

[disk]
label= Home:
color=#ffffff
separator=false
command=/bin/bash $HOME/.libi3bview 17
interval=1

[disk]
label= Root:
color=#ffffff
separator=false
command=/bin/bash $HOME/.libi3bview 15
interval=1

