separator_block_width=30
markup=none
color=#ffffff

[source]
label= IP:
command=/bin/bash $HOME/.libi3bview 24
separator=false
interval=1

[sink]
label= OP:
command=/bin/bash $HOME/.libi3bview 26
separator=false
interval=1

[cpufreq]
label= Freq:
command=/bin/bash $HOME/.libi3bview 27
separator=false
interval=1

[usedram]
label= RAM:
command=/bin/bash $HOME/.libi3bview 21
separator=false
interval=1

[disk]
label= Home:
separator=false
command=/bin/bash $HOME/.libi3bview 17
interval=1

[disk]
label= Root:
separator=false
command=/bin/bash $HOME/.libi3bview 15
interval=1

[download_total]
label= DL: 
command=/bin/bash $HOME/.libi3bview 14
separator=false
interval=1

[upload_total]
label= UL:
command=/bin/bash $HOME/.libi3bview 13
separator=false
interval=1

[active_tcp_connection_count]
label= TCPA:
command=/bin/bash $HOME/.libi3bview 12
separator=false
interval=1

[listening_tcp_count]
label= TCPL:
command=/bin/bash $HOME/.libi3bview 11
separator=false
interval=1

[date]
label= 
command=/bin/bash $HOME/.libi3bview 09
separator=false
interval=1

[time]
label= 
command=/bin/bash $HOME/.libi3bview 08
separator=false
interval=1

